<script>
  function logout(event) {
    event.preventDefault();

    fetch('/logout', {
        method: 'POST'
      })
      .then(response => {
        if (response.ok) {
          window.location.href = '/userLoginScreen';
        }
      })
      .catch(error => {
        console.log(error);
      });
  }

  function navigateTo(route) {
    window.location.href = route;
  }
